execute if score $curr.player map.value = %init map.const run bossbar set src:online name {"text": "Aucun joueurs connectés", "color": "gray"}
execute if score $curr.player map.value > %init map.const unless score $curr.player map.value = %x.player map.const run bossbar set src:online name [{"text": "Joueurs connectés", "color": "aqua"}, {"text": ": ", "color": "yellow"}, {"score": {"name": "$curr.player", "objective": "map.value"}, "color": "green"}, {"text": "/", "color": "white"}, {"score": {"name": "%x.player", "objective": "map.const"}, "color": "red"}]
execute if score %more.player map.value > %x.player map.const run bossbar set src:online name [{"text": "ERREUR", "color": "red"}, {"text": ": ", "color": "yellow"}, {"text": "Nombres de joueurs incorect", "color": "gold"}]
execute if score $curr.player map.value = %x.player map.const unless score %more.player map.value > %x.player map.const run bossbar set src:online name {"text": "SERVEUR COMPLET", "color": "red", "bold": true}
